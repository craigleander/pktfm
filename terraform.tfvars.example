# Example terraform.tfvars file
# Copy this to terraform.tfvars and update with your actual values

aws_region   = "us-east-1"
vpc_id       = "vpc-xxxxxxxxxxxxxxxxx"  # Replace with your VPC ID
subnet_ids   = ["subnet-xxxxxxxxx", "subnet-yyyyyyyyy"]  # Replace with your subnet IDs
project_name = "myapp"
environment  = "qa"  # or "prod"

allowed_cidrs = [
  "10.0.0.0/16"  # Replace with your VPC CIDR or application subnet CIDRs
]

db_port = 5432

tags = {
  Project     = "MyApplication"
  ManagedBy   = "Terraform"
  Environment = "qa"
}
